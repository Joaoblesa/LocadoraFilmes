generator client {
  provider = "prisma-client-js"
  // Mantenha seu output customizado se você precisar dele
  output   = "../generated/prisma" 
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Genero { 
  id Int @id @default(autoincrement())
  nome String @db.VarChar(100)
  descricao String? @db.VarChar(200) // Assumindo os campos do seu controller
  @@map("tbl_genero") 
}

model tbl_teste {
  id Int @id @default(autoincrement())
  nome String
}
